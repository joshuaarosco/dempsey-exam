{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "ext-pcntl": "^7.2",
        "ext-posix": "^7.2",
        "aftership/aftership-php-sdk": "^5.1",
        "brozot/laravel-fcm": "^1.2",
        "doctrine/dbal": "^2.7",
        "dusterio/link-preview": "^1.2",
        "enniel/laravel-fcm-notification-channel": "^0.0.1",
        "fabpot/goutte": "^3.2",
        "gloudemans/shoppingcart": "^2.5",
        "hieu-le/active": "^3.4",
        "intervention/image": "^2.3",
        "ixudra/curl": "6.*",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "5.4.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.2",
        "league/fractal": "0.12.*@dev",
        "maddhatter/laravel-fullcalendar": "^1.2",
        "milon/barcode": "^5.2",
        "netshell/paypal": "dev-master",
        "oureastudios/laravel5-braintree": "dev-master",
        "payeezy/payeezy-php": "dev-master",
        "predis/predis": "^1.1",
        "propaganistas/laravel-phone": "^4.0",
        "pusher/pusher-chatkit-server": "^0.5.6",
        "pusher/pusher-php-server": "^3.3",
        "sammyk/laravel-facebook-sdk": "^3.5",
        "simplesoftwareio/simple-qrcode": "~2",
        "srmklive/paypal": "~1.0",
        "tymon/jwt-auth": "0.5.*",
        "barryvdh/laravel-dompdf": "0.8.2",
        "maatwebsite/excel": "~2.1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Api\\": "app-dev/Api/",
            "Core\\": "app-dev/Core/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "ext-pcntl": "7.2",
            "ext-posix": "7.2"
        }
    }
}
